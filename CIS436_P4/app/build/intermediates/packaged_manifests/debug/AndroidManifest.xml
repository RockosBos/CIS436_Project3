            android:name="com.example.cis436_p4.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.CIS436_P4.NoActionBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>